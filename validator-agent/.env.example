# --- Environment mode ---------------------------------------------------------
# "development" (default) = InMemory services, AI Studio
# "production" = VertexAi sessions, GCS artifacts, Vertex AI models
ENVIRONMENT=development

# --- Google AI Studio (development) ------------------------------------------
GOOGLE_GENAI_USE_VERTEXAI=FALSE
GOOGLE_API_KEY=your_api_key_here

# --- Google Cloud / Vertex AI (production) -----------------------------------
# GOOGLE_GENAI_USE_VERTEXAI=TRUE
# GOOGLE_CLOUD_PROJECT=your_project_id
# GOOGLE_CLOUD_LOCATION=us-central1
# AGENT_ENGINE_ID=1234567890123456789  # Numeric ID only, not the full resource name
# GCS_ARTIFACT_BUCKET=your-artifact-bucket

# --- Server ------------------------------------------------------------------
# CORS_ORIGINS=http://localhost:3000
# LOG_LEVEL=INFO
